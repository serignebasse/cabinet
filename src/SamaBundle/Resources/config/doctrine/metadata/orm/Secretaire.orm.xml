<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Secretaire" table="secretaire">
    <id name="idsecretaire" type="integer" column="idSecretaire">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomsecretaire" type="string" column="nomSecretaire" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenomsecretaire" type="string" column="prenomSecretaire" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telsecretaire" type="decimal" column="telSecretaire" precision="10" scale="0" nullable="true"/>
    <field name="emailsecretaire" type="string" column="emailSecretaire" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sexesecretaire" type="string" column="sexeSecretaire" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addressesecretaire" type="string" column="addresseSecretaire" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
